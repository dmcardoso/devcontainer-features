{
    "id": "dotfiles",
    "version": "0.0.5",
    "name": "Fish with starship and vim dotfiles",
    "description": "A feature to configure fish shell and vim",
    "installsAfter": [
        "ghcr.io/devcontainers-extra/features/fish-apt-get",
        "ghcr.io/devcontainers-extra/features/starship"
    ],
    "options": {
        "homeUser": {
            "default": "vscode",
            "description": "Home user",
            "proposals": [
                "vscode",
                "root"
            ],
            "type": "string"
        }
    }
}
